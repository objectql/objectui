{
  "name": "@object-ui/example-crm",
  "version": "1.0.0",
  "description": "CRM Metadata Example using ObjectStack Protocol",
  "private": true,
  "type": "module",
  "main": "dist/objectstack.json",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./objectstack.config": "./objectstack.config.ts",
    "./plugin": "./plugin.ts"
  },
  "scripts": {
    "dev": "objectstack serve --dev objectstack.config.ts",
    "serve": "objectstack serve objectstack.config.ts",
    "build": "objectstack compile objectstack.config.ts",
    "start": "tsx server.ts"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@objectstack/core": "^3.0.4",
    "@objectstack/driver-memory": "^3.0.4",
    "@objectstack/objectql": "^3.0.4",
    "@objectstack/plugin-auth": "^3.0.4",
    "@objectstack/plugin-hono-server": "^3.0.4",
    "@objectstack/runtime": "^3.0.4",
    "@objectstack/spec": "^3.0.4",
    "hono": "^4.11.9",
    "pino": "^8.21.0",
    "pino-pretty": "^13.1.3"
  },
  "devDependencies": {
    "@objectstack/cli": "^3.0.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
